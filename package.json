{
  "name": "order-app",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "lint:strict": "eslint --max-warnings=0 src",
    "prettier": "prettier --write */**/*.{js,jsx,json,ts,tsx,md}",
    "stylelint": "stylelint \"**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "typecheck": "tsc --noEmit --incremental false",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "prepare": "husky install",
    "types": "tsc --noEmit",
    "sort:packages": "node scripts/sortPackages.mjs",
    "check-versions": "node scripts/checkPackageVersions.mjs",
    "postinstall": "npm run check-versions"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ],
    "*.{css,scss}": "stylelint --fix"
  },
  "dependencies": {
    "@emotion/react": "11.13.0",
    "@emotion/styled": "11.13.0",
    "@mui/icons-material": "6.0.1",
    "@mui/lab": "6.0.0-beta.9",
    "@mui/material": "6.0.2",
    "@mui/styled-engine-sc": "6.0.1",
    "@mui/x-data-grid": "7.15.0",
    "@mui/x-date-pickers": "7.15.0",
    "@reduxjs/toolkit": "2.2.7",
    "@types/react-redux": "7.1.33",
    "@vitejs/plugin-react": "4.3.1",
    "child_process": "1.0.2",
    "classnames": "2.5.1",
    "dayjs": "1.11.13",
    "fs": "0.0.1-security",
    "js-cookie": "3.0.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.52.2",
    "react-redux": "9.1.2",
    "react-router-dom": "6.26.0",
    "styled-components": "6.1.13",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-scss": "6.4.1",
    "usehooks-ts": "3.1.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/react-input-mask": "3.0.5",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-perf": "3.3.2",
    "eslint-plugin-react-refresh": "0.4.7",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-unused-imports": "3.2.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "sass": "1.77.6",
    "stylelint": "16.6.1",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-sass-guidelines": "11.1.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "5.0.0",
    "typescript": "5.5.4",
    "vite": "5.3.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
